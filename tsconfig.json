{
  "baseUrl": ".",
  "include": ["./src"],
  "exclude": ["dist", "public"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": false,
    "noEmitOnError": false,
    "allowJs": true,
    "resolveJsonModule": true,
    "types": ["node", "jest", "@testing-library/jest-dom", "webpack"],
    "typeRoots": ["./node_modules/@types"]
  }
}
